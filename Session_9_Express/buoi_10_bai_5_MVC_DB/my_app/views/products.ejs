<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Product List</h1>  
    <table border="1">
        <thead>
            <tr>
                <th>Id</th>
                <th>SideLength</th>
                <th>Perimeter</th>
                <th>Area</th>
            </tr>
        </thead>
        <tbody>
        <% products.forEach(product => { %>  
            <tr>
                <td>
                    <%= product.id %>
                </td>
                <td>
                    <%= product.sideLength %>
                </td>
                <td>
                    <%= product.perimeter %>
                </td>
                <td>
                    <%= product.area %>
                </td>
            </tr>
        <% }) %>
        </tbody>
    </table>
</body>
</html>